{
  "manifest_version": 3,
  "name": "Oasa Telematics Real Time",
  "version": "1.0",
  "description": "Converts minutes to real-time in Oasa Telematics.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://telematics.oasa.gr/*"],
  "content_scripts": [
    {
      "matches": ["https://telematics.oasa.gr/*"],
      "js": ["content.js"]
    }
  ]
}